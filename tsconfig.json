{
  "compilerOptions": {
    "lib": ["ES2023"],
    "module": "node16",
    "target": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "rootDir": "src",
    "outDir": "dist",
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true
  },
  "include": [
    "src"
  ]
}
